<h1>店舗投稿フォーム</h1>
<%= form_with model: @food_image, local:true do |f| %>

  <h4>画像</h4>
  <%= f.attachment_field :image %>
  <h4>店舗名</h4>
  <%= f.text_field :shop_name %>
  <h4>投稿タイトル</h4>
  <%= f.text_field :title %>
  <h4>説明</h4>
  <%= f.text_area :caption %>
  <h4>アクセス</h4>
  <%= f.text_field :address, placeholder: "住所" %>
  <h4>営業時間</h4>
  <%= f.text_field :hour %>
  <h4>休業日</h4>
  <%= f.text_field :holiday %>
  <%= f.submit '投稿' %>
<% end %>